<h1>ブログ一覧</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
  <% @blogs.each do |blog| %>
    <tr>
      <td><%= blog.title %></td>
      <td><%= blog.content %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to '投稿', new_blog_path %>